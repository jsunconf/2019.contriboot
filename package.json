{
  "name": "contriboot-2019",
  "version": "0.0.1",
  "description": "Contributions & Interests",
  "scripts": {
    "start": "yarn run build && yarn run copy:dist && concurrently 'yarn run watch' 'http-server ./build/ -o -c-1 -a localhost --cors'",
    "watch": "concurrently 'yarn run watch:css' 'yarn run watch:js'",
    "watch:js": "webpack --env.NODE_ENV=local --watch",
    "watch:css": "stylus --inline --watch --sourcemap-inline -u autoprefixer-stylus assets/styles/jsunconf.contriboot.styl -o build/static",
    "watch:images": "yarn run build:images && watch-run -p 'assets/images/*' yarn run build:images",
    "clean": "rm -rf build && mkdir -p build/static/images",
    "clean:images": "rm -rf build/static/images && mkdir -p build/static/images",
    "lint": "eslint ./assets/scripts",
    "copy:dist": "cp assets/index.html build",
    "build": "yarn run clean && yarn run build:css && yarn run copy:dist && webpack",
    "build:js": "tsc --project .",
    "build:css": "stylus --inline --compress -u autoprefixer-stylus assets/styles/jsunconf.contriboot.styl -o build/static",
    "build:images": "yarn run clean:images && cp -r assets/images/* build/static/images/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/marked": "^0.6.3",
    "@types/node": "8.9.4",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/to-markdown": "^3.0.0",
    "autoprefixer-stylus": "^0.14.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "marked": "^0.6.0",
    "stylus": "^0.54.5",
    "stylus-normalize": "^3.0.3",
    "to-markdown": "^3.1.1",
    "ts-loader": "^5.3.3",
    "watch-run": "^1.2.5",
    "watchify": "^3.11.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "firebase": "^5.7.3",
    "react-markdown": "^4.0.6",
    "react-medium-editor": "^1.8.1",
    "react-spinner": "^0.2.7",
    "reactfire": "^1.0.0",
    "react": "16",
    "react-dom": "^16.8.4",
    "typescript": "^3.3.3333"
  }
}
